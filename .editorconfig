# Editor configuration, see https://editorconfig.org
root = true

[*]
end_of_line = crlf
indent_style = space
indent_size = 2
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
max_line_length = 140
tab_width = 2

[*.md]
max_line_length = off
trim_trailing_whitespace = false
