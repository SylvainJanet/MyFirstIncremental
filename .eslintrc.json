{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "plugins": ["html", "@typescript-eslint"],
  "extends": ["eslint:all", "plugin:@typescript-eslint/all", "eslint-config-prettier", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "tsconfig.json"
  },
  "rules": {
    "no-console": "off"

